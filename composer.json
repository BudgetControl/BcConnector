{
    "name": "budgetcontrol/libs-connector",
    "description": "defines the dependencies required for a PHP library in a Budget control application",
    "type": "none",
    "license": "MIT",
    "version": "0.0.1",
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "7.9.x-dev"
    },
    "require-dev": {
        "nikic/php-parser": "^5.0",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Budgetcontrol\\Connector\\": "src/"
        },
        "files": [
            "config/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marco De Felice",
            "email": "marco.defelice@mlabfactory.it"
        }
    ],
    "minimum-stability": "beta"
}
